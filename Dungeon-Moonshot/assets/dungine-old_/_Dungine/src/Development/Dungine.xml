        <member name="T:Dungine.Common.GameDetail">
            <summary>
            The <c>GameDetail</c> object contains various game details, including:
            <list type="bullet">
                <item>
                    <term>Title</term>
                    <description>The game title.</description>                
                </item>
                <item>
                    <term>SubTitle</term>
                    <description>The game subtitle.</description>                
                </item>
                <item>
                    <term>Genre</term>
                    <description>The game genre.</description>                
                </item>
                <item>
                    <term>MinPlayer</term>
                    <description>Minimum number of players.</description>                
                </item>
                <item>
                    <term>MaxPlayer</term>
                    <description>Maximum number of players.</description>                
                </item>
            </list>
        </summary>
        </member>
        <member name="P:Dungine.Common.GameDetail.Title">
            <summary>
            The game title.
            <para>
                <example>Example: <c>"Dungeon"</c></example>
            </para>
        </summary>
        </member>
        <member name="P:Dungine.Common.GameDetail.SubTitle">
            <summary>
            The game sub-title.
            <para>
                <example>Example: <c>"A Scary Adventure"</c></example>
            </para>
        </summary>
        </member>
        <member name="P:Dungine.Common.GameDetail.Genre">
            <summary>
            The game genre, which can be:
            <list type="bullet">
                <item>
                    <term>text-adventure</term>
                    <description>A text adventure game</description>                
                </item>
            </list>
            <para>
                <example>Example: <c>"text-adventure"</c></example>
            </para>
        </summary>
        </member>
        <member name="P:Dungine.Common.GameDetail.MinPlayer">
            <summary>
            The minimum number of players.
            <para>
                <example>Example: <c>"1"</c></example>
            </para>
        </summary><remarks>
            The minimum number of players must be &gt; 0
        </remarks>
        </member>
        <member name="P:Dungine.Common.GameDetail.MaxPlayer">
            <summary>
            The maximum number of players.
            <para>
                <example>Example: <c>"3"</c></example>
            </para>
        </summary><remarks>
            The maximum number of players must be &lt; 5
        </remarks>
        </member>
